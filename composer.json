{
    "name": "ziadoz/assert-html",
    "description": "A library for performing test assertions on HTML.",
    "homepage": "https://github.com/ziadoz/assert-html",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ziadoz\\AssertHtml\\": "src/",
            "Ziadoz\\AssertHtml\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Jamie York",
            "email": "ziadoz@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-dom": "*",
        "ext-libxml": "*",
        "symfony/var-dumper": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3"
    }
}
